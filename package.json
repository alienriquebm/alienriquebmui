{
  "name": "alienriquebmui",
  "version": "1.1.9",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./style": {
      "import": "./dist/index.css",
      "default": "./dist/index.css"
    }
  },
  "scripts": {
    "build": "postcss src/styles.css -o dist/index.css && tsup"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "esbuild": "^0.24.0",
    "postcss-cli": "^11.0.0",
    "react-aria-components": "^1.3.3"
  }
}
